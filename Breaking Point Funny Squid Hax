	wait(0.5)local ba=Instance.new("ScreenGui")
	local ca=Instance.new("TextLabel")local da=Instance.new("Frame")
	local _b=Instance.new("TextLabel")local ab=Instance.new("TextLabel")ba.Parent=game.CoreGui
	ba.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;ca.Parent=ba;ca.Active=true
	ca.BackgroundColor3=Color3.new(0.176471,0.176471,0.176471)ca.Draggable=true
	ca.Position=UDim2.new(0.1,0,0.01,0)ca.Size=UDim2.new(0,1,0,1)
	ca.Font=Enum.Font.SourceSansSemibold;ca.Text="Anti Afk Kick Script"ca.TextColor3=Color3.new(0,1,1)
	ca.TextSize=0.1;da.Parent=ca
	da.BackgroundColor3=Color3.new(0.1,0.1,0.1)da.Position=UDim2.new(0,0,1.01,0)
	da.Size=UDim2.new(0,1,0,1)_b.Parent=da
	_b.BackgroundColor3=Color3.new(0.1,0.1,0.17)_b.Position=UDim2.new(0,0,0.8,0)
	_b.Size=UDim2.new(0,2,0,1)_b.Font=Enum.Font.Arial;_b.Text="Made by Airwav"
	_b.TextColor3=Color3.new(1,1,1)_b.TextSize=0.1;ab.Parent=da
	ab.BackgroundColor3=Color3.new(0.176471,0.176471,0.176471)ab.Position=UDim2.new(0,0,0.15,0)
	ab.Size=UDim2.new(0,1,0,1)ab.Font=Enum.Font.ArialBold;ab.Text="Status: Script Started"
	ab.TextColor3=Color3.new(1,1,1)ab.TextSize=0.1;local bb=game:service'VirtualUser'
	game:service'Players'.LocalPlayer.Idled:connect(function()
		bb:CaptureController()bb:ClickButton2(Vector2.new())
		ab.Text="ROBLOX kick attempt has been aborted"wait(2)ab.Text="Script Re-Enabled"end)
end)

