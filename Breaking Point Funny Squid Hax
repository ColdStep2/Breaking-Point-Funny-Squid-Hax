fullbright.Name = "fullbright"
fullbright.Parent = main
fullbright.BackgroundColor3 = Color3.fromRGB(12, 13, 11)
fullbright.BorderColor3 = Color3.fromRGB(27, 51, 255)
fullbright.Position = UDim2.new(0.314347178, 0, 0.148835331, 0)
fullbright.Size = UDim2.new(0, 86, 0, 26)
fullbright.Font = Enum.Font.TitilliumWeb
fullbright.Text = "Full Bright"
fullbright.TextColor3 = Color3.fromRGB(83, 255, 73)
fullbright.TextSize = 29.000
fullbright.MouseButton1Down:connect(function()
	local Light = game:GetService("Lighting")

	function dofullbright()
		Light.Ambient = Color3.new(1, 1, 1)
		Light.ColorShift_Bottom = Color3.new(1, 1, 1)
		Light.ColorShift_Top = Color3.new(1, 1, 1)
	end

	dofullbright()

	Light.LightingChanged:Connect(dofullbright)
end)

