noclip.Name = "noclip"
noclip.Parent = main
noclip.BackgroundColor3 = Color3.fromRGB(12, 13, 11)
noclip.BorderColor3 = Color3.fromRGB(27, 51, 255)
noclip.Position = UDim2.new(0.312749386, 0, 0.301613152, 0)
noclip.Size = UDim2.new(0, 86, 0, 26)
noclip.Font = Enum.Font.TitilliumWeb
noclip.Text = "Noclip"
noclip.TextColor3 = Color3.fromRGB(83, 255, 73)
noclip.TextSize = 30.000
noclip.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
	game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
	while true do
		game:GetService("RunService").Stepped:wait()
		game.Players.LocalPlayer.Character.Head.CanCollide = false
		game.Players.LocalPlayer.Character.Torso.CanCollide = false
	end
end)

infjump.Name = "infjump"
infjump.Parent = main
infjump.BackgroundColor3 = Color3.fromRGB(12, 13, 11)
infjump.BorderColor3 = Color3.fromRGB(27, 51, 255)
infjump.Position = UDim2.new(0.135627598, 0, 0.44324398, 0)
infjump.Size = UDim2.new(0, 86, 0, 22)
infjump.Font = Enum.Font.TitilliumWeb
infjump.Text = "Inf Jump"
infjump.TextColor3 = Color3.fromRGB(83, 255, 73)
infjump.TextSize = 30.000
infjump.MouseButton1Down:connect(function()
	local InfiniteJumpEnabled = true
	game:GetService("UserInputService").JumpRequest:connect(function()
		if InfiniteJumpEnabled then
			game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
		end
	end)
end)

bighitbox.Name = "bighitbox"
bighitbox.Parent = main
bighitbox.BackgroundColor3 = Color3.fromRGB(12, 13, 11)
bighitbox.BorderColor3 = Color3.fromRGB(27, 51, 255)
bighitbox.Position = UDim2.new(0.515701354, 0, 0.44324398, 0)
bighitbox.Size = UDim2.new(0, 86, 0, 22)
bighitbox.Font = Enum.Font.TitilliumWeb
bighitbox.Text = "Big Hitbox"
bighitbox.TextColor3 = Color3.fromRGB(83, 255, 73)
bighitbox.TextSize = 28.000
bighitbox.MouseButton1Down:connect(function()
	_G.HeadSize = 8
	_G.Disabled = true

	game:GetService('RunService').RenderStepped:connect(function()
		if _G.Disabled then
			for i,v in next, game:GetService('Players'):GetPlayers() do
				if v.Name ~= game:GetService('Players').LocalPlayer.Name then
					pcall(function()
						v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
						v.Character.HumanoidRootPart.Transparency = 0.7
						v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Green")
						v.Character.HumanoidRootPart.Material = "Neon"
						v.Character.HumanoidRootPart.CanCollide = false
					end)
				end
			end
		end
	end)
end)
